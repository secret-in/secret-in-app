{
  "name": "secretin-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://secret-in.me",
  "main": "electron.js",
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "electron": "^1.4.13",
    "electron-packager": "^8.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^1.6.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gh-pages": "^0.12.0",
    "node-sass": "^4.5.2",
    "raw-loader": "^0.5.1",
    "react-scripts": "^0.9.5",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "alt": "^0.18.6",
    "alt-utils": "^1.0.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.5",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mousetrap": "^1.6.1",
    "qrcode.react": "0.6.1",
    "react": "15.4.1",
    "react-bootstrap": "0.30.7",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "15.4.1",
    "react-overlays": "0.6.10",
    "react-router": "4.0.0-alpha.6",
    "secretin": "^1.5.0",
    "url-join": "^2.0.0",
    "uuid": "^3.0.1"
  },
  "scripts": {
    "deploy": "scripts/deploy.sh",
    "start": "yarn run build-css && yarn run watch-css & NODE_PATH=./src react-scripts start",
    "build": "yarn run build-css && NODE_PATH=./src react-scripts build",
    "build-css": "node-sass src/stylesheets/application.sass src/index.css",
    "watch-css": "node-sass src/stylesheets/application.sass src/index.css -w",
    "electron": "yarn run build && electron-packager ./ --overwrite --version=1.4.13",
    "lint": "node_modules/.bin/eslint src/",
    "prettier": "prettier --print-width 80 --trailing-comma es5 --single-quote --write \"src/**/*.js\"",
    "test": "yarn run lint && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.js"
  }
}
